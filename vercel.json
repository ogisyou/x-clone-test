{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://us-central1-x-clone-test2.cloudfunctions.net/api/:path*"
    },
    {
      "source": "/((?!_next/|static/).*)",
      "destination": "/$1"
    }
  ]
}